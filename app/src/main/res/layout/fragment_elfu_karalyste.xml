<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".view.kingdoms.views.ElfuKaralysteFragment">

        <ImageView
            android:id="@+id/imageViewElfKingdomBackground"
            android:layout_width="411dp"
            android:layout_height="731dp"
            android:scaleType="centerCrop"
            android:src="@drawable/eflu_karalyste_empty"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageViewElfuNamasElfuKaralyste"
            android:layout_width="320dp"
            android:layout_height="363dp"
            android:layout_marginStart="112dp"
            android:layout_marginTop="224dp"
            android:src="@drawable/elf_home"
            android:visibility="invisible"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageViewVandensLelija"
            android:layout_width="127dp"
            android:layout_height="96dp"
            android:layout_marginStart="60dp"
            android:src="@drawable/vandens_lelija"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/imageViewElfeLiepa"
            app:layout_constraintTop_toTopOf="@+id/imageViewElfeLiepa"
            app:layout_constraintVertical_bias="0.519" />

        <ImageView
            android:id="@+id/imageViewDrakonasElfuKaralyste"
            android:layout_width="109dp"
            android:layout_height="159dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="48dp"
            android:src="@drawable/drakonas"
            android:visibility="invisible"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageViewStebuklingasPaukstis" />

        <ImageView
            android:id="@+id/imageViewElfasJonasElfuKaralyste"
            android:layout_width="163dp"
            android:layout_height="276dp"
            android:layout_marginStart="140dp"
            android:layout_marginBottom="16dp"
            android:src="@drawable/elfas"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/imageViewElfeLiepa"
            android:layout_width="85dp"
            android:layout_height="222dp"
            android:layout_marginEnd="68dp"
            android:layout_marginBottom="16dp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@drawable/elf_girl" />

        <ImageView
            android:id="@+id/imageViewStebuklingasPaukstis"
            android:layout_width="127dp"
            android:layout_height="238dp"
            android:layout_marginTop="88dp"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.152"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/bird_magestic" />

        <ImageView
            android:id="@+id/imageViewMagiskaGele"
            android:layout_width="111dp"
            android:layout_height="327dp"
            android:layout_marginTop="84dp"
            android:layout_marginEnd="4dp"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@+id/imageViewElfasJonasElfuKaralyste"
            app:layout_constraintTop_toTopOf="@+id/imageViewDrakonasElfuKaralyste"
            app:srcCompat="@drawable/gele_elfu_karalyste" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>