        <ImageView
            android:id="@+id/imageView66"
            android:layout_width="137dp"
            android:layout_height="320dp"
            android:layout_marginTop="64dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView58"
            app:srcCompat="@drawable/nykstukas_naujas" />

        <ImageView
            android:id="@+id/imageView67"
            android:layout_width="104dp"
            android:layout_height="312dp"
            android:layout_marginStart="88dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView66"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.126"
            app:srcCompat="@drawable/bird_gnom_kingdom" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>