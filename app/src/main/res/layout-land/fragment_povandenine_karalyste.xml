<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/povandenine_karalyste_background"
        tools:context=".view.kingdoms.views.PovandenineKaralysteFragment">


        <ImageView
            android:id="@+id/imageViewPilisPovandenineKaralyste"
            android:layout_width="303dp"
            android:layout_height="477dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/castle_povandenine_karalyste_6"
            android:visibility="invisible"
            />

        <ImageView
            android:id="@+id/imageViewAuksineZuvele"
            android:layout_width="101dp"
            android:layout_height="152dp"
            android:layout_marginTop="76dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/golden_fish"
            android:visibility="invisible"
            />

        <ImageView
            android:id="@+id/imageViewZuvyteMelynojiGupija"
            android:layout_width="107dp"
            android:layout_height="93dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="4dp"
            app:layout_constraintStart_toEndOf="@+id/imageViewZuvyteRaudonasisNeonas"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/blue_purple_fish"
            android:visibility="invisible"
            />

        <ImageView
            android:id="@+id/imageViewZuvytePurpurineTetra"
            android:layout_width="102dp"
            android:layout_height="104dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="92dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/colorful_fish"
            android:visibility="invisible"

            />

        <ImageView
            android:id="@+id/imageViewZuvyteRaudonasisNeonas"
            android:layout_width="104dp"
            android:layout_height="141dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="48dp"
            app:layout_constraintBottom_toTopOf="@+id/imageViewJurosGeles"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/orange_colorful_fish"
            android:visibility="invisible"
            />

        <ImageView
            android:id="@+id/imageViewJuruArkliukas"
            android:layout_width="115dp"
            android:layout_height="168dp"
            android:layout_marginStart="144dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageViewJurosGeles"
            app:layout_constraintTop_toTopOf="@+id/imageViewGorgonaceaAugalas"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/sea_horse"
            android:visibility="invisible"
            />

        <ImageView
            android:id="@+id/imageViewUndine"
            android:layout_width="165dp"
            android:layout_height="181dp"
            android:layout_marginTop="32dp"
            app:layout_constraintEnd_toStartOf="@+id/imageViewAnacharisAugalas"
            app:layout_constraintHorizontal_bias="0.767"
            app:layout_constraintStart_toEndOf="@+id/imageViewGorgonaceaAugalas"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/mermaid_little"
            android:visibility="invisible"

            />

        <ImageView
            android:id="@+id/imageViewAnacharisAugalas"
            android:layout_width="120dp"
            android:layout_height="158dp"
            android:layout_marginTop="24dp"
            android:layout_marginBottom="19dp"
            android:visibility="invisible"
            app:layout_constraintBottom_toTopOf="@+id/imageViewKrabas"
            app:layout_constraintEnd_toStartOf="@+id/imageViewAuksineZuvele"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/imageViewPilisPovandenineKaralyste"
            app:layout_constraintTop_toBottomOf="@+id/imageViewZuvytePurpurineTetra"
            app:srcCompat="@drawable/papartis" />

        <ImageView
            android:id="@+id/imageViewGorgonaceaAugalas"
            android:layout_width="69dp"
            android:layout_height="225dp"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@+id/imageViewPilisPovandenineKaralyste"
            app:layout_constraintStart_toEndOf="@+id/imageViewZuvyteRaudonasisNeonas"
            app:layout_constraintTop_toBottomOf="@+id/imageViewZuvyteMelynojiGupija"
            app:srcCompat="@drawable/purple_sea_plant" />

        <ImageView
            android:id="@+id/imageViewJurosGeles"
            android:layout_width="91dp"
            android:layout_height="194dp"
            android:layout_marginBottom="4dp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageViewGorgonaceaAugalas"
            app:layout_constraintHorizontal_bias="0.341"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/colorful_plant" />

        <ImageView
            android:id="@+id/imageViewKrabas"
            android:layout_width="94dp"
            android:layout_height="82dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="12dp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageViewAuksineZuvele"
            app:layout_constraintVertical_bias="0.91"
            app:srcCompat="@drawable/crab" />

        <ImageView
            android:id="@+id/imageViewMoliuskas"
            android:layout_width="100dp"
            android:layout_height="86dp"
            android:layout_marginBottom="3dp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageViewJuruArkliukas"
            app:layout_constraintStart_toEndOf="@+id/imageViewJurosGeles"
            app:layout_constraintTop_toBottomOf="@+id/imageViewGorgonaceaAugalas"
            app:srcCompat="@drawable/shell" />

        <ImageView
            android:id="@+id/imageViewAstunkojis"
            android:layout_width="146dp"
            android:layout_height="127dp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageViewKrabas"
            app:layout_constraintHorizontal_bias="0.486"
            app:layout_constraintStart_toEndOf="@+id/imageViewJuruArkliukas"
            app:layout_constraintTop_toBottomOf="@+id/imageViewUndine"
            app:layout_constraintVertical_bias="0.408"
            app:srcCompat="@drawable/cute_creature_povandenine_karalyste" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</layout>
