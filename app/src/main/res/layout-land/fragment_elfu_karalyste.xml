<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/eflu_karalyste_empty"
        app:layout_constraintBottom_toTopOf="@+id/imageView14"
        app:layout_constraintTop_toTopOf="@+id/imageView14"
        tools:context=".view.kingdoms.views.ElfuKaralysteFragment">


        <ImageView
            android:id="@+id/imageViewElfuNamasElfuKaralyste"
            android:layout_width="341dp"
            android:layout_height="357dp"
            android:layout_marginBottom="120dp"
            android:src="@drawable/elf_home"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageViewStebuklingasPaukstis"
            app:layout_constraintStart_toEndOf="@+id/imageViewDrakonasElfuKaralyste"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageViewVandensLelija"
            android:layout_width="191dp"
            android:layout_height="102dp"
            android:layout_marginTop="76dp"
            android:layout_marginEnd="40dp"
            android:src="@drawable/vandens_lelija"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageViewElfeLiepa" />

        <ImageView
            android:id="@+id/imageViewDrakonasElfuKaralyste"
            android:layout_width="122dp"
            android:layout_height="313dp"
            android:layout_marginEnd="340dp"
            android:src="@drawable/drakonas"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@+id/imageViewStebuklingasPaukstis"
            app:layout_constraintHorizontal_bias="0.844"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageViewStebuklingasPaukstis" />

        <ImageView
            android:id="@+id/imageViewElfasJonasElfuKaralyste"
            android:layout_width="125dp"
            android:layout_height="220dp"
            android:layout_marginStart="16dp"
            android:src="@drawable/elfas"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageViewDrakonasElfuKaralyste"
            app:layout_constraintTop_toTopOf="@+id/imageViewDrakonasElfuKaralyste"
            app:layout_constraintVertical_bias="0.894" />

        <ImageView
            android:id="@+id/imageViewElfeLiepa"
            android:layout_width="238dp"
            android:layout_height="176dp"
            android:layout_marginStart="56dp"
            android:layout_marginTop="36dp"
            android:visibility="invisible"
            app:layout_constraintStart_toEndOf="@+id/imageViewElfasJonasElfuKaralyste"
            app:layout_constraintTop_toTopOf="@+id/imageViewElfasJonasElfuKaralyste"
            app:srcCompat="@drawable/elf_girl" />

        <ImageView
            android:id="@+id/imageViewStebuklingasPaukstis"
            android:layout_width="204dp"
            android:layout_height="178dp"
            android:layout_marginTop="72dp"
            android:layout_marginEnd="20dp"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/bird_magestic" />

        <ImageView
            android:id="@+id/imageViewMagiskaGele"
            android:layout_width="151dp"
            android:layout_height="111dp"
            android:layout_marginStart="4dp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageViewElfasJonasElfuKaralyste"
            app:layout_constraintTop_toTopOf="@+id/imageViewElfeLiepa"
            app:layout_constraintVertical_bias="0.888"
            app:srcCompat="@drawable/gele_elfu_karalyste" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>